{
  "expo": {
    "name": "gabaritei",
    "slug": "gabaritei",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#407BFF",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#407BFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.gabaritei"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#407BFF",
        "foregroundImage": "./assets/images/logo.png"
      },
      "package": "com.anonymous.gabaritei"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageResizeMode": "contain",
          "backgroundColor": "#407BFF"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
